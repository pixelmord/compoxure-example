{
  "server": {
    "port": 7000,
    "host": "localhost"
  },
  "backend": [
    {
      "fn": "selectGoogle",
      "dontPassUrl": true,
      "target":"https://www.google.com"
    },
    {
      "pattern": ".*",
      "dontPassUrl": false,
      "target":"{{server:example-service}}"
    }
  ],
  "statusCodeHandlers": {
    "403":{
      "fn": "handleUnauthorised",
      "data": {
        "url": "http://localhost:7000/login"
      }
    }
  }
}
